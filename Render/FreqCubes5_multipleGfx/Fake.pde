    float[] axes = fakePointsTimed.get(nextIdx);
    angleAxis1 = a1;
    angleAxis2 = a2;
    angleAxis3 = a3;
    angleAxis4 = a4;
    angleAxis5 = a5;
    angleAxis6 = a6;
    nextIdx += 1;
    readPositionsFromFile();    
  }
}

void nextRobotTimed2() {
  int t = millis() - t0 + delay;
  if (t >= nextTime) {
    println("got point at "+ t + " " + nextIdx);
    float[] axes = fakePointsTimed.get(nextIdx);
    angleAxis1 = radians(axes[1]);
